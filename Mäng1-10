 pakkumine = prompt("Sisesta number")
let Proovinud = 0 
let vastus = Math.floor(Math.random() * 10) + 1
console.log(vastus)
while (pakkumine != vastus)
{
    pakkumine = prompt("Sisesta number")
    Proovinud++
    console.log('on proovinud', Proovinud )
    if (Proovinud > 2)
        break
    if (pakkumine!=vastus)
    alert("Sorry, sisestatud arv ei ole 천ige  ")
    if (pakkumine == vastus -1 || pakkumine == vastus +1)
    alert("L채hedal oled")
    if (pakkumine == null)
        break 


}
if(pakkumine == vastus) 
console.log("Yay! Arvasid 천ige numbri 채ra!")
else console.log("Proovi uuesti")
